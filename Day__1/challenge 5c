#include<stdio.h>

int main()
{
    int etat;

    printf("entrer l'etat de l'eau: ");
    scanf("%d",&etat);

    if (etat < 0)
    {
        printf("Solid.");
    }
    else if(etat > 0 && etat < 100)
    {
        printf("Liquide.");
    }
    else 
    {
        printf("Gaz.");
    }

    
    
}